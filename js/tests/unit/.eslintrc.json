{
  "extends": [
    "../../../.eslintrc.json"
  ],
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "env": {
        "es6": false,
        "jquery": true,
        "qunit": true
      },
      "globals": {
        "bootstrap": false,
        "sinon": false,
        "Util": false,
        "Alert": false,
        "Button": false,
        "Carousel": false,
        "Simulator": false,
        "Toast": false
      },
      "rules": {
        // ES6
        "no-var": "off",
        "object-shorthand": "off",
        "prefer-arrow-callback": "off",
        "prefer-template": "off",
        "prefer-rest-params": "off"
      }
    }
  ]
}
